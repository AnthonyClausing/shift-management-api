{
    "root": true,
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint/eslint-plugin"
    ],
    "rules": {
        "camelcase": "off",
        "no-unused-vars": "off",
        "space-before-function-paren": "off",
        "comma-dangle": "off",
        "spaced-comment": "off"
      }
}
